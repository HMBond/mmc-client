{
  "name": "modular-wmc-3000",
  "description": "A modular web-based midi controller.",
  "version": "0.3.0",
  "author": "Mike Bond",
  "license": "ISC",
  "main": "public/electron.js",
  "scripts": {
    "dev": "vite --host",
    "build-web": "tsc --noEmit && vite build",
    "preview": "vite preview --host",
    "build": "tsc --noEmit && vite build && electron-builder --config electron-builder.yml",
    "build-electron": "electron-builder --config electron-builder.yml --dir",
    "lint": "eslint --config '.eslintrc.js'",
    "electron-dev": "concurrently dev electron-dev-task",
    "electron-dev-task": "wait-on http://169.254.18.178:3000 && electron ."
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.4",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webmidi": "^3.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.2",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^6.5.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.28.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "wait-on": "^6.0.0"
  }
}
