{
  "name": "modular-midi-controller",
  "description": "A modular web-based midi controller.",
  "version": "0.4.0",
  "author": "Mike Bond",
  "license": "ISC",
  "main": "public/electron.js",
  "scripts": {
    "dev": "run-p lint-watch typescript-watch dev-web",
    "dev-web": "vite",
    "dev-electron": "electron .",
    "preview": "vite preview --host",
    "build": "tsc && vite build && electron-builder --config electron-builder.yml -mwl",
    "build-web": "tsc && vite build",
    "build-electron": "electron-builder --config electron-builder.yml --dir",
    "lint": "eslint",
    "lint-watch": "esw -w --ext .tsx",
    "typescript-watch": "tsc -w",
    "test": "jest --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.4",
    "electron-is-dev": "^2.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webmidi": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.2",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-jest": "^27.4.5",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-watch": "^8.0.0",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
